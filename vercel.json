{
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ],
    "ignoreFiles": [
        ".venv",
        ".env",
        "*.md",
        "__pycache__/",
        "*.zip",
        ".gitignore",
        "needs.txt",
        "output.png",
        "Dockerfile",
        ".dockerignore"
    ]
}